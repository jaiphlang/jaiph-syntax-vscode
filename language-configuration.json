{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ]
  ],
  "autoClosingPairs": [
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "\"",
      "close": "\""
    },
    {
      "open": "'",
      "close": "'"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "[",
      "close": "]"
    }
  ],
  "surroundingPairs": [
    [
      "{",
      "}"
    ],
    [
      "\"",
      "\""
    ],
    [
      "'",
      "'"
    ],
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*(rule|workflow)\\b.*\\{\\s*$",
      "end": "^\\s*\\}\\s*$"
    }
  }
}
